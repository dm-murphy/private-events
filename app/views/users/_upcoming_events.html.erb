<h3>Upcoming Events:</h3>
  <% upcoming_invites = @user.attended_events.where("date >= ?", Time.current) %>
  <% upcoming_invites.each do |event| %>
    <li>Event Date: <%= event.date %></li>
    <li>Event Name: <%= event.title %></li>
    <li>Description: <%= event.description %></li>
    <%= link_to 'View Event', event_path(event.id) %>
    </br>
    </br>
  <% end %>